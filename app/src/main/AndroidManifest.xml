<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="anuson.komkid.permitgeographypro">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashScreen" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity   android:name=".MainActivity" android:screenOrientation="portrait"/>
        <activity   android:name=".Register_user" android:screenOrientation="portrait"/>

        <activity   android:name=".Menu_farmer" android:screenOrientation="portrait"/>
        <activity   android:name=".Menu_farmer_1" android:screenOrientation="portrait"/>
        <activity   android:name=".Menu_farmer_2" android:screenOrientation="portrait"/>
        <activity   android:name=".Menu_farmer_3" android:screenOrientation="portrait"/>


        <activity   android:name=".Menu_user" android:screenOrientation="portrait"/>
        <activity   android:name=".Menu_user_1" android:screenOrientation="portrait"/>
        <activity   android:name=".Menu_user_2" android:screenOrientation="portrait"/>
        <activity   android:name=".Menu_user_3" android:screenOrientation="portrait"/>

    </application>

</manifest>